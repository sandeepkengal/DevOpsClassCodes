pipeline{
    agent any

    stages{
        stage('code-checkout'){
            steps{
            git credentialsId: 'githubcred', url: 'https://github.com/sandeepkengal/DevOpsClassCodes.git'
        }
    }

        stage('code-cpmpile'){
            steps{
                sh "mvn compile"
            }
        }

        stage('code-testing'){
            steps{
                sh "mvn test"
            }
        }

        stage('quality-asuarance'){
            steps{
                sh "mvn pmd:pmd"
            }
        }
}

}
